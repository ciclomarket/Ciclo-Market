{
  "name": "mundobike",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/optimize-public-images.mjs",
    "start": "npm run server",
    "dev": "vite",
    "dev:admin": "vite --config admin/vite.config.ts",
    "build": "npm run build:web && npm run build:admin",
    "build:web": "vite build",
    "postbuild": "node scripts/optimize-public-images.mjs",
    "build:admin": "vite build --config admin/vite.config.ts",
    "preview": "vite preview",
    "preview:admin": "vite preview --config admin/vite.config.ts --port 5274",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "server": "node server/src/index.js",
    "review-reminder:once": "node -e \"require('./server/src/jobs/reviewReminder').runReviewReminderOnce(Number(process.env.REVIEW_REMINDER_BATCH_LIMIT)||50).then(()=>process.exit(0)).catch((e)=>{console.error(e);process.exit(1)})\"",
    "optimize:images": "node scripts/optimize-public-images.mjs",
    "supabase:image-matrix": "node scripts/supabase-image-matrix.mjs",
    "saved-search:digest:once": "node -e \"require('./server/src/jobs/savedSearchDigest').runSavedSearchDigestOnce().then((n)=>{console.log('[digest] sent',n);process.exit(0)}).catch((e)=>{console.error(e);process.exit(1)})\"",
    "campaign:trustblast": "node server/scripts/campaigns/sendTrustLevelBlast.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "browser-image-compression": "^2.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase": "^12.3.0",
    "framer-motion": "^12.33.0",
    "heic2any": "^0.0.4",
    "lucide-react": "^0.563.0",
    "mercadopago": "^2.9.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^23.11.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.26.2",
    "resend": "^3.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.3.4"
  }
}
